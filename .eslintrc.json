{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "webextensions": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "new-cap": 0,
    "comma-dangle": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}
