{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_title": "Thought Crime",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "icons": {
     "16": "icons/tc-16.png",
     "32": "icons/tc-32.png",
     "128": "icons/tc-128.png"
   },
   "description": "Easily block distracting or annoying websites and boost your productivity.",
   "manifest_version": 2,
   "name": "Thought Crime",
   "options_page": "options.html",
   "permissions": [ "storage", "tabs", "webNavigation", "webRequest", "<all_urls>", "webRequestBlocking", "notifications" ],
   "version": "0.0.2"
}
